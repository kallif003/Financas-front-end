		"husky": "^8.0.3",
		"lint-staged": "^13.2.3",
		"prettier": "^3.0.0"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-customizable"
		}
	},
	"lint-staged": {
		"**/*.{js,jsx,ts,tsx}": [
			"eslint --fix",
			"prettier --write"
