		"autoprefixer": "10.4.14",
		"axios": "^1.4.0",
		"eslint": "^8.0.1",
		"eslint-config-next": "13.4.10",
		"next": "13.4.10",
		"postcss": "8.4.26",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-spinners": "^0.13.8",
		"tailwindcss": "3.3.3",
		"typescript": "*",
		"windstitch": "^0.3.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.6.6",
		"@commitlint/config-conventional": "^17.6.6",
